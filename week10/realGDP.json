{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.2.0.json",

"title": {
    "text": "Real Gross Domestic Product ",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },

  "data": {
    "url": "https://raw.githubusercontent.com/qt19623/qt19623.github.io/main/week10/rawrealGDP.json",
    "format":{
        "type":"json",
      "property": "observations"
      }
  },

  "height": 300,
  
  "width": 280,
  
  "mark": {"type": "line"},
  
  "encoding": {
    "x": {
      "field": "date", 
      "type":"temporal"},
    "y": {
      "field": "value",
      "type": "quantitative"
    },
    
    "color": {
      "field": "countryiso3code",
      "type": "nominal"
    }
  }
}
